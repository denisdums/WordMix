{
  "name": "app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "remix:build": "remix vite:build ",
    "remix:dev": "tsx watch --ignore ./vite.config.ts.*.mjs --clear-screen=false ./server.ts vite:build --watch\n",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "build": "npm run graphql:compile  && npm run remix:build",
    "dev": "npm run graphql:compile && npm run remix:dev",
    "graphql:compile": "graphql-codegen-esm --config app/config/codegen/codegen.ts"
  },
  "dependencies": {
    "@_tw/themejson": "^0.2.0",
    "@apollo/client": "^3.9.8",
    "@graphql-tools/schema": "^10.0.3",
    "@remix-run/express": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@wordpress/block-library": "^8.30.0",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "html-react-parser": "^5.1.9",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "nprogress": "^0.2.0",
    "qs": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/graphql-modules-preset": "^4.0.6",
    "@graphql-codegen/typed-document-node": "^5.0.6",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@remix-run/dev": "^2.8.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.37",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
